Proceso CalculeElPromedioDeUnEstudianteQueCursa5Materias
	definir m1,m2,m3,m4,m5,promedio Como Real;
	Escribir "Nota del estudiante m1";
	leer m1;
	Escribir "Nota del estudiante m2";
	leer m2;
	Escribir "Nota del estudiante m3";
	leer m3;
	Escribir "Nota del estudiante m4";
	leer m4;
	Escribir "Nota del estudiante m5";
	leer m5;
	promedio<-(m1+m2+m3+m4+m5)/5;
	Escribir "promedio";
	Escribir promedio;
	
	
FinProceso
